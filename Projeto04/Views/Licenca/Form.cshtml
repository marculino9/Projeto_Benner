@{
    Layout = "~/Views/Shared/_AdmLayout.cshtml";
}
@model Projeto02.Models.Licenca

<h3 class="form- active">Cadastrar Licenca</h3>
<div>
    <form action="/Licenca/Adiciona" method="post">
        @Html.ValidationSummary(true)
        <label for="chave">Chave:</label>
        @*<input type="text" name="Licenca.chave" id="chave" class="form-control" />*@
        @Html.EditorFor(model => model.Chave)
        @Html.ValidationMessageFor(model => model.Chave)
        <br />
        <label for="quantidade">Quantidade:</label>
        @*<input type="text" name="Licenca.quantidade" id="quantidade" class="form-control" />*@
        @Html.EditorFor(model => model.Quantidade)
        @Html.ValidationMessageFor(model => model.Quantidade)
        <br />
        <label for="SoftwareId">SoftwareId:</label>
        <select id="SoftwareId" name="Licenca.SoftwareId" class="form-control">
            @foreach (var software in ViewBag.Software)
            {
                <option value="@software.Id" selected="@software.Id.Equals(ViewBag.SoftwareId)">
                    @software.NomeSoftware
                </option>

            }
        </select>
        @Html.ValidationMessageFor(model => model.SoftwareId)
        @*<br />
        <label for="usuarioId">UsuarioId:</label>
        <input type="text" name="usuarioId" id="usuarioId" class="form-control" />*@
        <br />
        <input class="btn btn-primary" type="submit" value="Cadastrar" />

    </form>
</div>
